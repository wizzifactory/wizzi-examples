{
	name "${app.name}"
	version "${app.version}"
    description "${app.description}"
	author "${app.author}"
	license "${app.license}"
	
    { scripts
		test "echo \"Error: no test specified\" && exit 1"
    	$include v07/app/package/webpack/scripts
	
    { dependencies
		$include v07/app/package/webpack/dependencies

	{ devDependencies
		$include v07/app/package/webpack/devDependencies

    $*
    name "${app.name}"
    description "${data.package.description}"
    version "${data.package.version}"
    author "${data.package.author}"
    license "${data.package.license}"

    { scripts
        $if app.Build.webpack.simple
            start "webpack-dev-server --mode development --open"
            build "webpack --mode production"
        $if app.Build.webpack.baseAndProd
            start "webpack-dev-server --mode development --config ./webpack.base.config.js --open --hot --history-api-fallback --env.PLATFORM=local --env.VERSION=stag"
            prebuild "webpack --mode production --config ./webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress"
            build "node server"
        $if app.Build.useLint
            lint "eslint --debug src/"
            lint:write "eslint --debug src/ --fix"
        $if app.Build.useTest && app.Build.test.useJest
            test "jest"
            test:watch "jest --watch"
            test:coverage "jest --coverage --colors"
        $if app.Build.usePrettier
            prettier "prettier --write src/**/*.js"

    $if app.Build.useLint && app.Build.lint.useHusky
        { husky
            { hooks
                pre-commit "lint-staged"
        { lint-staged
            [ *.(js|jsx)
                "npm run lint:write" 
                "git add" 

    { dependencies
        $if app.Build.react.materialUI
            @material-ui/core "^3.0.0"
            @material-ui/icons "^3.0.0"
        bootstrap "^4.1.3"
        prop-types "^15.6.2"
        react "^16.6.1"
        react-fontawesome "^1.6.1"
        react-dom "^16.6.1"
        $if app.Build.react.redux
            react-redux "^5.0.7"
        $if app.Build.react.router
            react-router-dom "^4.3.1"
        $if app.Build.react.redux
            redux "^4.0.0"
            redux-thunk "^2.3.0"
        $if app.Build.useSocketIO
            socket.io-client "^2.2.0"
        $if app.Build.react.styledComponents
            styled-components "^3.3.3"
        $if app.Build.react.transition
            react-transition-group "^2.4.0"

    { devDependencies
        @babel/core "^7.0.0"
        @babel/plugin-proposal-class-properties "^7.0.0"
        @babel/plugin-proposal-export-namespace-from "^7.0.0"
        @babel/plugin-proposal-throw-expressions "^7.0.0"
        @babel/plugin-syntax-dynamic-import "^7.0.0"
        @babel/polyfill "^7.0.0-beta.51"
        @babel/preset-env "^7.0.0-beta.51"
        @babel/preset-react "^7.0.0-beta.51"
        $if app.Build.useLint
            babel-eslint "^8.2.3"
        $if app.Build.useTest && app.Build.test.useJest
            babel-jest "^23.4.2"
        babel-loader "^8.0.0-beta.0"
        copy-webpack-plugin "^4.5.1"
        css-loader "^0.28.11"
        $if app.Build.useTest && app.Build.test.useEnzime
            enzyme "^3.3.0"
            enzyme-adapter-react-16 "^1.1.1"
        $if app.Build.useLint
            eslint "^4.19.1"
            eslint-config-airbnb "^17.0.0"
            $if app.Build.useTest && app.Build.test.useJest
                eslint-config-jest-enzyme "^6.0.2"
            eslint-plugin-babel "^5.1.0"
            eslint-plugin-import "^2.12.0"
            $if app.Build.useTest && app.Build.test.useJest
                eslint-plugin-jest "^21.18.0"
            eslint-plugin-jsx-a11y "^6.0.3"
            $if app.Build.usePrettier
                eslint-plugin-prettier "^2.6.0"
            eslint-plugin-react "^7.9.1"
        html-webpack-plugin "^3.2.0"
        $if app.Build.useTest && app.Build.test.useJest
            jest "^23.4.2"
        $if app.Build.useLint
            lint-staged "^7.3.0"
        mini-css-extract-plugin "^0.4.3"
        node-sass "^4.8.3"
        optimize-css-assets-webpack-plugin "^4.0.0"
        $if app.Build.usePrettier
            husky "^1.1.2"
        $if app.Build.usePrettier
            prettier "^1.14.3"
        sass-loader "^7.0.3"
        style-loader "^0.21.0"
        uglifyjs-webpack-plugin "^1.2.5"
        webpack "^4.12.0"
        webpack-cli "^3.0.8"
        webpack-dev-server "^3.1.4"
        webpack-merge "^4.1.3"
        webpack-visualizer-plugin "^0.1.11"
    *$        